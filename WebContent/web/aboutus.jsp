<script type="text/javascript">
$(window).scroll(function(){
	var scrollheight =$(document).scrollTop()+$(window).height()//窗口卷去的高度-窗口的高度
	$('.jumbo').each(function(){
		jumboHeight= $(this).offset().top;//元素距离文档顶部的高度
		if(scrollheight>=jumboHeight){//卷去的高度>元素距离文档顶部的高度 与 默认值=0
			$(this).animate({margin:"0"},1000)
		}
	})
	$('.jumbo-box').each(function(){
		jumboHeight= $(this).offset().top;//元素距离文档顶部的高度
		if(scrollheight>=jumboHeight){//卷去的高度>元素距离文档顶部的高度 与 默认值=0
			$(this).find('.swiper-container').animate({margin:"0"},1000)
		}
	})
});
$(window).scroll();
$('.swiper-container').each(function(){//自适应添加轮播效果
	var swiperLength = $(this).find('.swiper-slide').length;
	var swiperId = $(this).attr('id');
	if(swiperLength>4){
		$(this).find('.iconfont').show();
		var mySwiper = new Swiper('#'+swiperId,{
			autoplay : 5000,
			loop: true,
			grabCursor : true,
			slidesPerView : 4,
			autoplayDisableOnInteraction : false,
			calculateHeight : true,
		});
		$('#'+swiperId+'btn1').click(function(){
			mySwiper.swipePrev(); 
		})
		$('#'+swiperId+'btn2').click(function(){
			mySwiper.swipeNext(); 
		})
	}
})
//banner轮播
</script>
